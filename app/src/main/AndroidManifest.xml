<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.novytes.fuzzz.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppChooserActivity"
            android:label="@string/title_activity_app_chooser" >
        </activity>
        <activity
            android:name=".SelectTargetActivity"
            android:label="@string/title_activity_select_target" >
        </activity>
        <activity
            android:name=".TargetActivityChooser"
            android:label="@string/title_activity_target_activity_chooser" >
        </activity>
    </application>

</manifest>
